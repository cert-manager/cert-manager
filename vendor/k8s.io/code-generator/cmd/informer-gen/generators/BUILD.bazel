    importmap = "github.com/jetstack/cert-manager/vendor/k8s.io/code-generator/cmd/informer-gen/generators",
    importpath = "k8s.io/code-generator/cmd/informer-gen/generators",
    tags = ["manual"],
    visibility = ["//visibility:public"],
