load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = ["const.go"],
    importmap = "github.com/xenolf/lego/vendor/golang.org/x/net/internal/iana",
    importpath = "golang.org/x/net/internal/iana",
    visibility = ["//vendor/github.com/xenolf/lego/vendor/golang.org/x/net:__subpackages__"],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [":package-srcs"],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
