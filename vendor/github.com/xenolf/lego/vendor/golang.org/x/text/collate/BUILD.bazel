load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = [
        "collate.go",
        "index.go",
        "option.go",
        "sort.go",
        "tables.go",
    ],
    importmap = "github.com/xenolf/lego/vendor/golang.org/x/text/collate",
    importpath = "golang.org/x/text/collate",
    visibility = ["//visibility:public"],
    deps = [
        "//vendor/github.com/xenolf/lego/vendor/golang.org/x/text/internal/colltab:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/golang.org/x/text/language:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/golang.org/x/text/unicode/norm:go_default_library",
    ],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [
        ":package-srcs",
        "//vendor/github.com/xenolf/lego/vendor/golang.org/x/text/collate/build:all-srcs",
    ],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
