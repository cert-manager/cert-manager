load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = [
        "appengine.go",
        "default.go",
        "doc_go19.go",
        "doc_not_go19.go",
        "go19.go",
        "google.go",
        "jwt.go",
        "not_go19.go",
        "sdk.go",
    ],
    importmap = "github.com/xenolf/lego/vendor/golang.org/x/oauth2/google",
    importpath = "golang.org/x/oauth2/google",
    visibility = ["//visibility:public"],
    deps = [
        "//vendor/github.com/xenolf/lego/vendor/cloud.google.com/go/compute/metadata:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/golang.org/x/net/context:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/golang.org/x/oauth2:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/golang.org/x/oauth2/internal:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/golang.org/x/oauth2/jws:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/golang.org/x/oauth2/jwt:go_default_library",
    ],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [":package-srcs"],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
