load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = [
        "account.go",
        "contact.go",
        "domain.go",
        "goinwx.go",
        "nameserver.go",
    ],
    importmap = "github.com/xenolf/lego/vendor/github.com/smueller18/goinwx",
    importpath = "github.com/smueller18/goinwx",
    visibility = ["//visibility:public"],
    deps = [
        "//vendor/github.com/xenolf/lego/vendor/github.com/fatih/structs:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/github.com/kolo/xmlrpc:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/github.com/mitchellh/mapstructure:go_default_library",
    ],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [":package-srcs"],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
