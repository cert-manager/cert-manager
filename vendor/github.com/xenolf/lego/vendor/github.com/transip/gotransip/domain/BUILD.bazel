load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = [
        "api.go",
        "domain.go",
    ],
    importmap = "github.com/xenolf/lego/vendor/github.com/transip/gotransip/domain",
    importpath = "github.com/transip/gotransip/domain",
    visibility = ["//visibility:public"],
    deps = [
        "//vendor/github.com/xenolf/lego/vendor/github.com/transip/gotransip:go_default_library",
        "//vendor/github.com/xenolf/lego/vendor/github.com/transip/gotransip/util:go_default_library",
    ],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [":package-srcs"],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
