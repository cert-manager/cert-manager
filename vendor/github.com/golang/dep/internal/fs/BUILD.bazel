load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "go_default_library",
    srcs = [
        "fs.go",
        "rename.go",
        "rename_windows.go",
    ],
    importmap = "github.com/jetstack/cert-manager/vendor/github.com/golang/dep/internal/fs",
